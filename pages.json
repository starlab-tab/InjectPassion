{
	"easycom": {
		"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/tabbar/index/index",
			"style": {
				"navigationBarTextStyle": "white",
				"navigationStyle": "custom"
				// "enablePullDownRefresh": true
			}
		},
		{
			"path": "pages/tabbar/collect/collect",
			"style": {
				"navigationBarBackgroundColor": "#333333",
				"navigationBarTextStyle": "white",
				"navigationBarTitleText": "Collect"
			}
		},
		{
			"path": "pages/tabbar/analysis/analysis",
			"style": {
				"navigationBarBackgroundColor": "#333333",
				"navigationBarTextStyle": "white",
				"navigationBarTitleText": "Analysis"
			}
		},
		{
			"path": "pages/tabbar/bay/bay",
			"style": {
				"navigationBarTextStyle": "white",
				"navigationBarBackgroundColor": "#333333",
				"navigationBarTitleText": "Bay"
				// "navigationStyle": "custom",

			}
		}, {
			"path": "pages/tabbar/attention/attention",
			"style": {
				"navigationBarTextStyle": "white",
				"navigationBarBackgroundColor": "#333333",
				"navigationBarTitleText": "Attention"
			}
		}, {
			"path": "pages/world/world",
			"style": {}
		}, {
			"path": "pages/freebuf/detail",
			"style": {
				"navigationStyle": "custom"
			}
		}, {
			"path": "pages/anquanke/detail",
			"style": {
				"navigationStyle": "custom"
			}
		}, {
			"path": "pages/anquanke/360week",
			"style": {}
		}, {
			"path": "pages/xianzhi/detail",
			"style": {
				"navigationStyle": "custom"
			}
		}, {
			"path": "pages/trace/trace",
			"style": {
				"navigationStyle": "custom",
				"onReachBottomDistance": 50
			}
		}
	    ,{
            "path" : "pages/newsflash/newsflash",
            "style" : {}
        }
        ,{
            "path" : "pages/cve/detail",
            "style" : {}
        }
    ],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "InjectPassion",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
		"color": "#666",
		"selectedColor": "#8cb6c0",
		"backgroundColor": "#fff",
		"height":"70",
		"list": [{
			"pagePath": "pages/tabbar/index/index",
			"iconPath": "static/tabbar/ic-index.png",
			"selectedIconPath": "static/tabbar/ic-index-active.png"
		}, {
			"pagePath": "pages/tabbar/collect/collect",
			"iconPath": "static/tabbar/ic-collect.png",
			"selectedIconPath": "static/tabbar/ic-collect-active.png"
		}, {
			"pagePath": "pages/tabbar/attention/attention",
			"iconPath": "static/tabbar/ic-attention.png",
			"selectedIconPath": "static/tabbar/ic-attention-active.png"
		}, {
			"pagePath": "pages/tabbar/analysis/analysis",
			"iconPath": "static/tabbar/ic-analysis.png",
			"selectedIconPath": "static/tabbar/ic-analysis-active.png"
		}, {
			"pagePath": "pages/tabbar/bay/bay",
			"iconPath": "static/tabbar/ic-bay.png",
			"selectedIconPath": "static/tabbar/ic-bay-active.png"
		}]
	},
    "condition" : { //模式配置，仅开发期间生效
        "current": 0, //当前激活的模式(list 的索引项)
        "list": [
            {
                "name": "", //模式名称
                "path": "", //启动页面，必选
                "query": "" //启动参数，在页面的onLoad函数里面得到
            }
        ]
    }
}
